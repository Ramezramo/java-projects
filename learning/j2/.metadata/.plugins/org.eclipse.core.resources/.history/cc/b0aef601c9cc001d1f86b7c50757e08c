package oop_in_java;

public class Car_2_54_prooo {
	String maker_name;
	int age;
	double weight;
	
	Car_2_54_prooo (String maker_name,int age,double weight){
		this.age = age;
		this.maker_name = maker_name;
		this.weight = weight;
	}
	
	void run() {
		System.out.println("i am running");
		
	}
	void stop() {
		System.out.println("stoping...");
		
	}
}
